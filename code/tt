#! /usr/bin/vvp
:ivl_version "10.1 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x564c47effa40 .scope module, "test" "test" 2 1;
 .timescale 0 0;
v0x564c47effbc0_0 .var "a", 31 0;
v0x564c47f1fca0_0 .var "b", 31 0;
v0x564c47f1fd80_0 .var "c", 5 0;
    .scope S_0x564c47effa40;
T_0 ;
    %vpi_call 2 8 "$dumpfile", "www.vcd" {0 0 0};
    %vpi_call 2 9 "$dumpvars" {0 0 0};
    %pushi/vec4 4294967295, 0, 32;
    %store/vec4 v0x564c47effbc0_0, 0, 32;
    %pushi/vec4 31, 0, 6;
    %store/vec4 v0x564c47f1fd80_0, 0, 6;
    %delay 0, 0;
    %load/vec4 v0x564c47effbc0_0;
    %ix/getv 4, v0x564c47f1fd80_0;
    %shiftr 4;
    %store/vec4 v0x564c47f1fca0_0, 0, 32;
    %vpi_call 2 13 "$display", "b = %b", v0x564c47f1fca0_0 {0 0 0};
    %delay 1, 0;
    %pushi/vec4 33, 0, 6;
    %store/vec4 v0x564c47f1fd80_0, 0, 6;
    %vpi_call 2 16 "$display", "c = %b", v0x564c47f1fd80_0 {0 0 0};
    %delay 50, 0;
    %load/vec4 v0x564c47effbc0_0;
    %ix/getv 4, v0x564c47f1fd80_0;
    %shiftr 4;
    %store/vec4 v0x564c47f1fca0_0, 0, 32;
    %vpi_call 2 19 "$display", "b = %b", v0x564c47f1fca0_0 {0 0 0};
    %end;
    .thread T_0;
    .scope S_0x564c47effa40;
T_1 ;
    %delay 100, 0;
    %vpi_call 2 22 "$finish" {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 3;
